<div class="container">
    <div>
        <div id="user-information">
            <p>Inloggad som: <%= @user.username %></p>
            <%= link_to 'Logga ut', logout_path %>
        </div>
        <h1>Applikationer</h1>
        <div>
            <ul id="user-clients">
                <% @clients.each do |client| %>
                    <li>
                        <h2><%= client.name %></h2>
                        <p><strong>Anv√§ndare:</strong> <%= client.user.username %></p>
                        <p><strong>API-nyckel:</strong> <%= client.key %> || <strong>Url:</strong> <%= client.url %></p>
                        <p><strong>Beskrivning:</strong> <%= client.description %></p>
                        <%= link_to 'Ta bort', admin_delete_client_path(:id => client.id) %>
                    </li>
                <% end %>
            </ul>
        </div>
    </div>
</div>